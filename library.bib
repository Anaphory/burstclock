  file = {/home/gereon/Zotero/storage/5JHXYW62/PNAS-2009-Traulsen-709-12.pdf;/home/gereon/Zotero/storage/KBMNDHFE/PNAS-2009-Traulsen-709-12.pdf;/home/gereon/Zotero/storage/2C82ZCE2/709.html;/home/gereon/Zotero/storage/E53D9W6G/709.html},
  keywords = {altruistic punishment,Cooperation,costly punishment,emergence,finite populations,Humans,mutation rates,prisoners-dilemma,public-goods,societies},
