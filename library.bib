  author = {Balbo, A. L. and Rubio-Campillo, X. and Rondelli, B. and Ramírez, M. and Lancelotti, C. and Torrano, A. and Salpeteur, M. and Lipovetzky, N. and Reyes-García, V. and Montañola, C. and Madella, M.},
