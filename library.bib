@article{dediu2011are,
  title = {Are {{Languages Really Independent}} from {{Genes}}? {{If Not}}, {{What Would}} a {{Genetic Bias Affecting Language Diversity Look Like}}?},
  shorttitle = {Are {{Languages Really Independent}} from {{Genes}}?},
  author = {Dediu, Dan},
