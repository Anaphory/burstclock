  shorttitle = {PSMED},
  author = {Barceló, Joan A. and Cuesta, J A and family=Castillo, given=Florencia, prefix=del, useprefix=true and family=Olmo, given=Ricardo, prefix=del, useprefix=true and Galán, José M and Mameli, Laura and Miguel, Francisco J and Poza, David and Santos, José I and Vilà, Xavier},
