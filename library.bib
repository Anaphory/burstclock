@article{huelsenbeck2004bayesian,
  title = {Bayesian {{Phylogenetic Model Selection Using Reversible Jump Markov Chain Monte Carlo}}},
  author = {Huelsenbeck, John P. and Larget, Bret and Alfaro, Michael E.},
