  shorttitle = {Complex {{Terrain Load Carriage Energy Expenditure Estimation Using Global Positioning System Devices}}},
  author = {Potter, Adam W. and Santee, William R. and Mullen, Stephen P. and Karis, Anthony J. and Blanchard, Laurie A. and Rome, Maxwell N. and Pitts, Kenneth P. and Looney, David P.},
