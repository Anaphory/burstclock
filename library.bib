  langid = {english},
  number = {10}
}

@article{hurford1989biological,
  title = {Biological evolution of the {{Saussurean}} sign as a component of the language acquisition device},
  author = {Hurford, James R.},
  date = {1989-02-01},
  journaltitle = {Lingua},
  shortjournal = {Lingua},
  volume = {77},
