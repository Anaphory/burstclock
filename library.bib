@article{tallavaara2012did,
  title = {Did the mid-{{Holocene}} environmental changes cause the boom and bust of hunter-gatherer population size in eastern {{Fennoscandia}}?},
  author = {Tallavaara, Miikka and Seppä, Heikki},
  date = {2012-02-01},
